{
   "author": "Joshua Foster",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon19.png"
      },
      "default_title": "URL Rewriter"
   },
   "description": "Rewrite URLs according to a custom regex when accessing a web page.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "64": "images/icon64.png"
   },
   "manifest_version": 2,
   "name": "URL Rewriter",
   "options_page": "options.html",
   "permissions": [
       "webNavigation",
       "storage",
       "tabs",
       "webRequest",
       "webRequestBlocking",
       "<all_urls>"
   ],
   "version": "0.13"
}
